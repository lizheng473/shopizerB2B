	<parameter name="ReportTitle" isForPrompting="true" class="java.lang.String">
		<parameterDescription><![CDATA[The title for the invoice, i.e. 'Invoice' or 'Quote'.]]></parameterDescription>
		<defaultValueExpression ><![CDATA[new String("Invoice")]]></defaultValueExpression>
	</parameter>
	<parameter name="InvoiceNumber" isForPrompting="true" class="java.lang.String">
		<parameterDescription><![CDATA[The invoice number to search for in the SugarCRM invoice module.]]></parameterDescription>
	</parameter>
	<parameter name="PaymentTerms" isForPrompting="true" class="java.lang.String">
		<parameterDescription><![CDATA[The payment terms for the invoice, i.e. Net 30 days]]></parameterDescription>
		<defaultValueExpression ><![CDATA[new String("Net 15 Days")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT 
invoice.date_sent AS InvoiceDate,
accounts.name AS CompanyName,
CONCAT(accounts.billing_address_street, '\n', accounts.billing_address_city, ', ', accounts.billing_address_state, ' ', accounts.billing_address_postalcode) AS CompanyAddress,
CONCAT(contacts.first_name, ' ', contacts.last_name) AS ContactName,
CONCAT(users.address_street, '\n', users.address_city, ', ', users.address_state, ' ', users.address_postalcode) AS MyAddress,
project_task.name AS Name, 
project.name AS Project, 
project_task.actual_effort AS Hours, 
users.hourly_rate_charged_to_client * project_task.actual_effort AS Cost
FROM project_task 
JOIN invoice ON project_task.invoice_id = invoice.id 
JOIN project ON project_task.parent_id = project.id 
JOIN users ON project_task.assigned_user_id = users.id 
JOIN contacts ON invoice.contact_id = contacts.id
JOIN accounts_contacts ON contacts.id = accounts_contacts.contact_id
JOIN accounts ON accounts_contacts.account_id = accounts.id
WHERE invoice.name = '$P!{InvoiceNumber}'
ORDER BY project.name;]]></queryString>

